function stepHit(step)
{
if (curSong == 'vessel')
   {
   if (step == 992)
      {
      FlxTween.tween(dad, {alpha: 0}, 1, {ease: FlxEase.linear});
      FlxTween.tween(camHUD, {alpha: 0}, 1.2, {ease: FlxEase.linear});
      FlxTween.tween(bg, {alpha: 0}, 1, {ease: FlxEase.circInOut});
      }
   if (step == 1099)
      {
      currentPlayState.dad = x;
      iconP2.switchAnim("lordxpa");
      FlxG.camera.zoom += 0.08;
      x.alpha = 1;
      FlxTween.tween(camHUD, {alpha: 1}, 0.1, {ease: FlxEase.linear});
      x.playAnim("intro", true);
      }
for (i in 1...7)
    {
    if (curStep == stepList[i])
       {
       onVoidEvent();
       }
    }
if (sequence == true && currentPlayState.health > 0.4)
   {
   currentPlayState.health -= 0.03 * 2;
   }
   }
}

