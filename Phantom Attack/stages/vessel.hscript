function start(song) {
    bg = new FlxSprite(0,0).loadGraphic(hscriptPath + 'welcome.png');
    bg.scrollFactor.set(0.7, 0.9);
    bg.antialiasing = true;
    boyfriend.alpha = 0;
    gf.alpha = 0;
    addSprite(bg, BEHIND_ALL);
    setDefaultZoom(0.69);
    dad.x = 910;
    dad.followCamX -= 150;
    dad.y = 170;
    boyfriend.followCamX -= 90;
    boyfriend.followCamY += 140;
    x = new Character(980, 180, 'lordxpa');
    x.alpha = 0.001;
    addSprite(x, BEHIND_NONE);
    chosenLetter = FlxG.random.int(1,25);
    chosenLetter2 = FlxG.random.int(1,25);
    chosenLetter3 = FlxG.random.int(1,25);
    chosenLetter4 = FlxG.random.int(1,25);
for (strum in enemyStrums)
    {
    strum.x -= 2000;
    }
}

